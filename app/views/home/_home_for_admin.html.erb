
<div class="row">
	<!-- Requested -->
	<div class="small-2 large-2">
		<div class="org_list">
			<!-- title -->
			<div class="title">
				<h4> Requested </h4>
			</div>

			<!-- content -->
				<br>
				<% @bgChecks_req.each do |bgcheck| %>
					<%= link_to bgcheck.individual.name, individual_path(bgcheck.individual.id) %><br> <br>
				<% end %>

		</div>
	</div>

	<!-- Criminal passed -->
	<div class="small-2 large-2">
		<div class="org_list">
			<!-- title -->
			<div class="title">
				<h4> Criminal Passed </h4>
			</div>

			<!-- content -->
				<br>
				<% @bgChecks_pCriminal.each do |bgcheck| %>
					<%= link_to bgcheck.individual.name, individual_path(bgcheck.individual.id) %><br> <br>
				<% end %>

		</div>
	</div>


</div>