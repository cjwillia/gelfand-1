<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Active</th>
      <th>Role</th>
      <th></th>
      <th></th>

    </tr>
  </thead>

  <tbody>
      <% @organization.memberships.each do |mem| %>
        <tr>
          <td><%= mem.individual.f_name %></td>
          <td><%= mem.individual.l_name %></td>
          <td><%= mem.individual.active %></td>
          <td><%= mem.individual.role %></td>

          <td><%= link_to 'Show', individual_path(mem.individual) %></td>
            <% if current_user.is_orgUser_for_specific_org(@organization) %>
               <td><%= link_to 'Remove', mem, method: :delete, data: { confirm: 'Are you sure?' } %></td>
            <% end %>
        </tr>
      <% end %>
  </tbody>
</table>

    